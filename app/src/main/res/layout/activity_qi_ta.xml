<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_qi_ta"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.hyyx.testdemo.activity.QiTaActivity">


    <include layout="@layout/common_title_bar"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:background="@color/white"
        android:orientation="vertical">

        <com.example.hyyx.testdemo.view.TitleWeek
            android:id="@+id/titleweek"
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <GridView
            android:id="@+id/gv_qita"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:listSelector="@null"
            android:numColumns="7"></GridView>
    </LinearLayout>


</LinearLayout>
