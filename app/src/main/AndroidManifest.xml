<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hyyx.testdemo">

    <!-- 申明网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 申明权限 操作SD卡 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.SecActivity" />
        <activity android:name=".activity.SelectPhotoActivity" />
        <activity android:name=".activity.PipeActivity" />
        <activity android:name=".activity.ShowDateActivity" />
        <activity android:name=".activity.ShowDate2Activity" />
        <activity android:name=".activity.RollActivity"></activity>
    </application>

</manifest>